generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Contact {
  id    Int     @id @default(autoincrement())
  name  String  @db.VarChar(100)
  phone String? @db.VarChar(20)
  email String? @db.VarChar(100)

  @@map("contacts")
}
